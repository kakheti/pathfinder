<% fiders = data %>
<div>
  <h3>
    0.4კვ ხაზები <code><%= fiders.total_entries %></code>
  </h3>
  <div class="compact-pagination">
    <%= will_paginate fiders %>
  </div>
</div>
<% if fiders.any? %>
  <table class="table table-bordered table-hover">
    <% fiders.each do |fider| %>
      <tr>
        <td>
          <strong>
            <a href="<%= objects_direction04_url(fider) %>">
              <%= fider.number %> - ს/ჯ #<%= fider.tp.name if fider.tp.present? %>
            </a>
          </strong>
          &mdash; <code><%= number_with_precision(fider.length*1000, precision: 0) unless fider.length.nil? %></code> მ
        </td>
      </tr>
    <% end %>
  </table>
<% else %>
  <%= render partial: '/data/no_data' %>
<% end %>