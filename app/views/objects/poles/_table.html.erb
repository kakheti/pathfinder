<% poles = data %>

<div>
  <h3>6-10კვ საყრდენები</h3>
  <div class="compact-pagination">
    <%= will_paginate poles %>
  </div>
</div>

<% if poles.any? %>
  <table class="table table-bordered table-hover">
    <% poles.each do |pole| %>
      <tr>
        <td>
          <a href="<%= objects_pole_url(pole) %>">
            6-10კვ საყრდენი #<strong><%= pole.name %></strong>
          </a>
        </td>
      </tr>
    <% end %>
  </table>
<% else %>
  <%= render partial: '/data/no_data' %>
<% end %>