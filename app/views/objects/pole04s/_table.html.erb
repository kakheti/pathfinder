<% poles = data %>

<div>
  <h3>0.4კვ ბოძები <code><%= poles.total_entries %></code></h3>
  <div class="compact-pagination">
    <%= will_paginate poles %>
  </div>
</div>

<% if poles.any? %>
  <table class="table table-bordered table-hover">
    <% poles.each do |pole| %>
      <tr>
        <td>
          <a href="<%= objects_pole04_url(pole) %>">
            0.4კვ ბოძი #<strong><%= pole.name %></strong>
          </a>
        </td>
      </tr>
    <% end %>
  </table>
<% else %>
  <%= render partial: '/data/no_data' %>
<% end %>